programa {
  funcao inicio() {

    //Declaração da vaviáveis
    real dividendo
    real divisor
    real resultado

    //Entrada de dados do usuário: Dividendo e Divisor
    escreva("Digite o dividendo: ")
    leia(dividendo)
    escreva("Digite o divisor: ")
    leia(divisor)


    //Cálculo da divisão
    resultado = dividendo/divisor
    
    //imprissão do resultado
    escreva(resultado)
  }
}
